  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "axios": "^0.21.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.0",
    "bootstrap": "^4.4.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "formik": "^2.0.6",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-mock-axios": "^3.2.0",
    "node-sass": "^4.13.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.23.0",
    "react": "^16.12.0",
    "react-content-loader": "^4.3.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-lazy-load": "^3.0.13",
    "react-linkify": "^1.0.0-alpha",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-waterfall": "^4.0.4",
    "react-youtube": "^7.9.0",
    "reactstrap": "^8.1.1",
    "recompose": "^0.30.0",
    "socket.io-client": "^2.3.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "3.7.2",
    "youtube-player": "^5.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.{ts,tsx}'",
    "eject": "react-scripts eject",
    "test:coverage": "jest --collect-coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/ramda": "^0.26.36",
    "@types/react-content-loader": "^4.0.0",
    "@types/react-helmet": "^5.0.14",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-youtube": "^7.6.2",
    "@types/reactstrap": "^8.0.6",
    "@types/recompose": "^0.30.7",
    "@types/socket.io-client": "^1.4.32",
    "@types/youtube-player": "^5.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript-babel-jest": "^1.0.6"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.(tsx|ts|js)$": "typescript-babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  }
}
